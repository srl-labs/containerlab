{
    "interfaces": {
        "access": [
            {
                "interface": "eth1",
                "type": "ipoe",
                "vlan-mode": "N:1"
            }
        ]
    },
    "dhcp": {
        "enable": true
    },
    "ipoe": {
        "ipv4": true,
        "ipv6": false
    },
    "sessions": {
        "count": 10,
        "start-rate": 2
    },
    "access-line": {
        "agent-circuit-id": "eth 0/1/{session-global}",
        "agent-remote-id": "BNG-Blaster-{session-global}"
    }
}
